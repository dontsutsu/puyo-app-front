<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<title>Tokopuyo</title>
	<link rel="stylesheet" href="static/css/main.css" />
</head>
<body>
	<div class="wrapper">
		<div class="container">
			<canvas id="tsumo" class="tsumo"></canvas>
			<canvas id="field" class="field"></canvas>
		</div>

		<div class="container">
			<canvas id="next" class="next"></canvas>
		</div>

		<div class="container">
			<%- include("partials/_timelineSetting") %>
		</div>

		<div class="container">
			<div class="keyboard-manual">
				<table>
					<thead>
						<tr>
							<th colspan="2">キーボード</th>
						</tr>
					</thead>
					<tbody>
						<tr>
							<th>Z</th>
							<td>左回転</td>
						</tr>
						<tr>
							<th>X</th>
							<td>右回転</td>
						</tr>
						<tr>
							<th>←</th>
							<td>左へ移動</td>
						</tr>
						<tr>
							<th>→</th>
							<td>右へ移動</td>
						</tr>
						<tr>
							<th>↓</th>
							<td>ぷよを落とす</td>
						</tr>
						<tr>
							<th>↑</th>
							<td>ひとつ戻す</td>
						</tr>
					</tbody>
				</table>
			</div>
		</div>
	</div>

	<script type="text/javascript" src="static/js/tokopuyo_bundle.js"></script>
</body>
</html>